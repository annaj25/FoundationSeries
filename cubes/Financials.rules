Skipcheck;

## Rule Financials-001: Populate value for Activity - Fed by Feeder Financials-001
['Activity'] = N: 
     IF(ELISANC('Account', 'Balance Sheet', !Account) <> 0
     , DB('Financials', !Currency, !Department, !Account, !Period, 'Ending Balance' ) - 
       DB('Financials', !Currency, !Department, !Account, DB('}ElementAttributes_Period', !Period, 'Prior Period'), 'Ending Balance' )
     , CONTINUE);

Feeders;

## Feeder Financials-001 - Feeds Rule Financials-001
['Ending Balance'] => DB('Financials', !Currency, !Department, !Account, !Period, 'Activity' );
['Ending Balance'] => DB('Financials', !Currency, !Department, !Account, DB('}ElementAttributes_Period', !Period, 'Next Period'), 'Activity' );


## Feeder Financials-E001 - Feeds Rule Forecast-Actuals
['Ending Balance'] => DB('Forecast', 'Actuals', !Currency, !Department, !Account, !Period, 'Forecast Entry');


